<!-- Traverse all subtiddlers with function pragma recursively -->

\function .children-closure(root)
	[all[tiddlers]parent<root>]
	:map:flat[.subtree<currentTiddler>]
\end

\function .subtree(root)
	[<root>]
	[.children-closure<root>]
\end