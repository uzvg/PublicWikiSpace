<!-- 使用说明：
* min ➡ low ➡ high ➡ max
* optimum: 最佳值
-->

\procedure twks-meter-bar(value,min,max,low,high,optimum,class)
	<meter value=<<value>>
		min=<<min>>
		max=<<max>>
		low=<<low>>
		high=<<high>>
		optimum=<<optimum>>
		class=<<class>>/>
\end